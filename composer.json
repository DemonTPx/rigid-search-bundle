{
  "name": "demontpx/rigid-search-bundle",
  "type": "symfony-bundle",
  "description": "Rigid search bundle for Symfony",
  "keywords": ["search", "symfony", "bundle"],
  "license": "MIT",
  "autoload": {
    "psr-4": { "Demontpx\\RigidSearchBundle\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Demontpx\\RigidSearchBundle\\": "tests/" }
  },
  "require": {
    "php": "^5.5|^7.0",
    "demontpx/util-bundle": "^0.1.6",
    "symfony/form": "^2.8|^3.0",
    "sensio/framework-extra-bundle": "^3.0",
    "knplabs/knp-paginator-bundle": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.3"
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
