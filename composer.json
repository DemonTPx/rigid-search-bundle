{
  "name": "demontpx/rigid-search-bundle",
  "type": "symfony-bundle",
  "description": "Rigid search bundle for Symfony",
  "keywords": ["search", "symfony", "bundle"],
  "license": "MIT",
  "autoload": {
    "psr-4": { "Demontpx\\RigidSearchBundle\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Demontpx\\RigidSearchBundle\\": "tests/" }
  },
  "require": {
    "php": "^7.1",
    "ext-simplexml": "*",
    "demontpx/util-bundle": "^0.5.1",
    "symfony/form": "^4.0|^5.0",
    "sensio/framework-extra-bundle": "^5.1",
    "knplabs/knp-paginator-bundle": "^2.5|^3.0|^4.0|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "symfony/yaml": "^4.0|^5.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
